# If you're using anything but the default, change this to
# point to your installation destination directory.
#
# Note, this Makefile assumes that rump kernel components are
# shared libs (.so).  For static libs (.a), please see Makefile.static
#
RD=../rump

all: webbrowser webbrowser_sockin

webbrowser: webbrowser.c
	cc -g -Wall -O0 $< -o $@ -I${RD}/include -L${RD}/lib -Wl,-R${RD}/lib -Wl,--no-as-needed -lrumpnet_virtif -lrumpnet_config -lrumpdev_bpf -lrumpnet_netinet -lrumpnet_net -lrumpnet -lrump

webbrowser_sockin: webbrowser.c
	cc -g -Wall -O3 $< -o $@ -DUSE_SOCKIN -I${RD}/include -L${RD}/lib -Wl,-R${RD}/lib -Wl,--no-as-needed -lrumpnet_sockin -lrumpnet -lrump

clean:
	rm -f webbrowser webbrowser_sockin *core
